#!/home/wade/ppbc/django/ppbc-venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'von-anchor==1.9.2','console_scripts','von_anchor_setnym'
__requires__ = 'von-anchor==1.9.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('von-anchor==1.9.2', 'console_scripts', 'von_anchor_setnym')()
    )
