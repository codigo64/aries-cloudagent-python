   "source": [
    "def id_to_seed(id):\n",
    "        id_len = len(str(id))\n",
    "        if id_len > 32:\n",
    "            raise Exception(\"max users reached!\")\n",
    "        return \"0\"*(32-id_len)+str(id)\n",
    "print(id_to_seed(500000000000000000000000000000000))"
